,WAAK,OAAL,CAAa,IAAb,CAAkB;AAAE,QAAA,IAAI,EAAE;AAAR,OAAlB;AACD,KAZM;AAaP,IAAA,cAAc,EAAE,wBAAU,IAAV,EAAgB;AAC9B,MAAA,YAAY,CAAC,OAAb,CAAqB,QAArB,EAA+B,IAA/B;AACA,MAAA,YAAY,CAAC,OAAb,CAAqB,UAArB,EAAiC,IAAI,CAAC,QAAtC;AACA,MAAA,YAAY,CAAC,OAAb,CAAqB,cAArB,EAAqC,IAAI,CAAC,YAA1C;AACA,MAAA,YAAY,CAAC,OAAb,CAAqB,eAArB,EAAsC,IAAI,CAAC,aAA3C;AACA,MAAA,KAAK,CAAC,uBAAD,CAAL;AACA,WAAK,UAAL;AACD,KApBM;AAqBP,IAAA,eAAe,EAAE,yBAAU,IAAV,EAAgB;AAC/B,MAAA,KAAK,CAAC,kBAAD,CAAL,CAD+B,CAE/B;AACD,KAxBM;AAyBP,IAAA,QAAQ,EAAE,oBAAY;AACpB,WAAK,OAAL,CAAa,IAAb,CAAkB;AAAE,QAAA,IAAI,EAAE;AAAR,OAAlB;AACD,KA3BM;AA4BP,IAAA,MAAM,EAAE,kBAAY;AAClB,MAAA,YAAY,CAAC,KAAb;AACA,MAAA,KAAK,CAAC,gBAAD,CAAL;AACA,WAAK,UAAL;AACD,KAhCM;AAiCP,IAAA,OAAO,EAAE,mBAAY;AACnB,WAAK,OAAL,CAAa,IAAb,CAAkB;AAAE,QAAA,IAAI,EAAE;AAAR,OAAlB;AACD;AAnCM,GARI;AA6Cb,EAAA,OAAO,EAAE,mBAAY;AACnB,SAAK,UAAL;AACD;AA/CY,CAAf","sourcesContent":["<template>\r\n  <div id=\"app\" class=\"app\">\r\n    <header>\r\n      <!-- <a class=\"navbar-brand col-md-3 col-lg-2 me-0 px-3\" href=\"#\">\r\n        CLAP Misión TIC\r\n      </a> -->\r\n      <div class=\"container\">\r\n        <div class=\"col-md-4 text-end\">\r\n          <button\r\n            class=\"btn btn-outline-primary me-2\"\r\n            v-if=\"is_auth\"\r\n            v-on:click=\"loadHome\"\r\n          >\r\n            Inicio\r\n          </button>\r\n          <button\r\n            class=\"btn btn-outline-primary me-2\"\r\n            v-if=\"is_auth\"\r\n            v-on:click=\"loadSignUp\"\r\n          >\r\n            Usuarios\r\n          </button>\r\n          <button\r\n            class=\"btn btn-outline-primary me-2\"\r\n            v-if=\"is_auth\"\r\n            v-on:click=\"loadIps\"\r\n          >\r\n            Ips\r\n          </button>\r\n          <button class=\"btn btn-primary\" v-if=\"is_auth\" v-on:click=\"logOut\">\r\n            Cerrar Sesión\r\n          </button>\r\n          <!-- <button v-if=\"!is_auth\" v-on:click=\"loadLogIn\" > Iniciar Sesión </button> -->\r\n        </div>\r\n      </div>\r\n    </header>\r\n\r\n    <div class=\"main-component\">\r\n      <router-view\r\n        v-on:completedLogIn=\"completedLogIn\"\r\n        v-on:completedSignUp=\"completedSignUp\"\r\n        v-on:logOut=\"logOut\"\r\n      >\r\n      </router-view>\r\n    </div>\r\n\r\n    <!-- Footer -->\r\n    <footer class=\"blockquote-footer\">\r\n      <span class=\"h6 text-uppercase\">By Group 6</span>\r\n    </footer>\r\n\r\n  </div>\r\n</template>\r\n\r\n\r\n\r\n\r\n<script>\r\nexport default {\r\n  name: \"App\",\r\n  data: function () {\r\n    return {\r\n      is_auth: false,\r\n    };\r\n  },\r\n  components: {},\r\n  methods: {\r\n    verifyAuth: function () {\r\n      this.is_auth = localStorage.getItem(\"isAuth\") || false;\r\n\r\n      if (this.is_auth == false) this.$router.push({ name: \"logIn\" });\r\n      else this.$router.push({ name: \"home\" });\r\n    },\r\n    loadLogIn: function () {\r\n      this.$router.push({ name: \"logIn\" });\r\n    },\r\n    loadSignUp: function () {\r\n      this.$router.push({ name: \"signUp\" });\r\n    },\r\n    completedLogIn: function (data) {\r\n      localStorage.setItem(\"isAuth\", true);\r\n      localStorage.setItem(\"username\", data.username);\r\n      localStorage.setItem(\"token_access\", data.token_access);\r\n      localStorage.setItem(\"token_refresh\", data.token_refresh);\r\n      alert(\"Autenticación Exitosa\");\r\n      this.verifyAuth();\r\n    },\r\n    completedSignUp: function (data) {\r\n      alert(\"Registro Exitoso\");\r\n      // this.completedLogIn(data);\r\n    },\r\n    loadHome: function () {\r\n      this.$router.push({ name: \"home\" });\r\n    },\r\n    logOut: function () {\r\n      localStorage.clear();\r\n      alert(\"Sesión Cerrada\");\r\n      this.verifyAuth();\r\n    },\r\n    loadIps: function () {\r\n      this.$router.push({ name: \"ips\" });\r\n    },\r\n  },\r\n  created: function () {\r\n    this.verifyAuth();\r\n  },\r\n};\r\n</script>\r\n\r\n\r\n\r\n\r\n\r\n\r\n<style>\r\nbody {\r\n  margin: 0 0 0 0;\r\n}\r\n.header {\r\n  margin: 0%;\r\n  padding: 0;\r\n  width: 100%;\r\n  height: 10vh;\r\n  min-height: 100px;\r\n  background-color: #283747;\r\n  color: #e5e7e9;\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n}\r\n.header h1 {\r\n  width: 20%;\r\n  text-align: center;\r\n}\r\n.header nav {\r\n  height: 100%;\r\n  width: 20%;\r\n  display: flex;\r\n  justify-content: space-around;\r\n  align-items: center;\r\n  font-size: 20px;\r\n}\r\n.header nav button {\r\n  color: #e5e7e9;\r\n  background: #283747;\r\n  border: 1px solid #e5e7e9;\r\n  border-radius: 5px;\r\n  padding: 10px 20px;\r\n}\r\n.header nav button:hover {\r\n  color: #283747;\r\n  background: #e5e7e9;\r\n  border: 1px solid #e5e7e9;\r\n}\r\n\r\n.main-component {\r\n  height: 75vh;\r\n  margin: 0%;\r\n  padding: 0%;\r\n  background: #fdfefe;\r\n}\r\n\r\n/* .footer {\r\n  margin: 0;\r\n  padding: 0;\r\n  width: 100%;\r\n  height: 10vh;\r\n  min-height: 100px;\r\n  background-color: #283747;\r\n  color: #e5e7e9;\r\n}\r\n.footer h2 {\r\n  width: 100%;\r\n  height: 100%;\r\n\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n} */\r\n</style>"],"sourceRoot":""}]}