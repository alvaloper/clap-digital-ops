V,EAAgB;AAC9B,MAAA,YAAY,CAAC,OAAb,CAAqB,QAArB,EAA+B,IAA/B;AACA,MAAA,YAAY,CAAC,OAAb,CAAqB,UAArB,EAAiC,IAAI,CAAC,QAAtC;AACA,MAAA,YAAY,CAAC,OAAb,CAAqB,cAArB,EAAqC,IAAI,CAAC,YAA1C;AACA,MAAA,YAAY,CAAC,OAAb,CAAqB,eAArB,EAAsC,IAAI,CAAC,aAA3C;AACA,MAAA,KAAK,CAAC,uBAAD,CAAL;AACA,WAAK,UAAL;AACD,KApBM;AAqBP,IAAA,eAAe,EAAE,yBAAU,IAAV,EAAgB;AAC/B,MAAA,KAAK,CAAC,kBAAD,CAAL,CAD+B,CAE/B;AACD,KAxBM;AAyBP,IAAA,QAAQ,EAAE,oBAAY;AACpB,WAAK,OAAL,CAAa,IAAb,CAAkB;AAAE,QAAA,IAAI,EAAE;AAAR,OAAlB;AACD,KA3BM;AA4BP,IAAA,MAAM,EAAE,kBAAY;AAClB,MAAA,YAAY,CAAC,KAAb;AACA,MAAA,KAAK,CAAC,gBAAD,CAAL;AACA,WAAK,UAAL;AACD,KAhCM;AAiCP,IAAA,OAAO,EAAE,mBAAY;AACnB,WAAK,OAAL,CAAa,IAAb,CAAkB;AAAE,QAAA,IAAI,EAAE;AAAR,OAAlB;AACD;AAnCM,GARI;AA6Cb,EAAA,OAAO,EAAE,mBAAY;AACnB,SAAK,UAAL;AACD;AA/CY,CAAf","sourcesContent":["<template>\n  <div id=\"app\" class=\"app\">\n    <header>\n      <!-- <a class=\"navbar-brand col-md-3 col-lg-2 me-0 px-3\" href=\"#\">\n        CLAP Misión TIC\n      </a> -->\n      <div class=\"container\">\n        <div class=\"col-md-4 text-end\">\n          <button\n            class=\"btn btn-outline-primary me-2\"\n            v-if=\"is_auth\"\n            v-on:click=\"loadHome\"\n          >\n            Inicio\n          </button>\n          <button\n            class=\"btn btn-outline-primary me-2\"\n            v-if=\"is_auth\"\n            v-on:click=\"loadSignUp\"\n          >\n            Usuarios\n          </button>\n          <button\n            class=\"btn btn-outline-primary me-2\"\n            v-if=\"is_auth\"\n            v-on:click=\"loadIps\"\n          >\n            Ips\n          </button>\n          <button class=\"btn btn-primary\" v-if=\"is_auth\" v-on:click=\"logOut\">\n            Cerrar Sesión\n          </button>\n          <!-- <button v-if=\"!is_auth\" v-on:click=\"loadLogIn\" > Iniciar Sesión </button> -->\n        </div>\n      </div>\n    </header>\n\n    <div class=\"main-component\">\n      <router-view\n        v-on:completedLogIn=\"completedLogIn\"\n        v-on:completedSignUp=\"completedSignUp\"\n        v-on:logOut=\"logOut\"\n      >\n      </router-view>\n    </div>\n\n    <!-- Footer -->\n    <footer class=\"blockquote-footer\">\n      <span class=\"h6 text-uppercase\">By Group 6</span>\n    </footer>\n\n  </div>\n</template>\n\n\n\n\n<script>\nexport default {\n  name: \"App\",\n  data: function () {\n    return {\n      is_auth: false,\n    };\n  },\n  components: {},\n  methods: {\n    verifyAuth: function () {\n      this.is_auth = localStorage.getItem(\"isAuth\") || false;\n\n      if (this.is_auth == false) this.$router.push({ name: \"logIn\" });\n      else this.$router.push({ name: \"home\" });\n    },\n    loadLogIn: function () {\n      this.$router.push({ name: \"logIn\" });\n    },\n    loadSignUp: function () {\n      this.$router.push({ name: \"signUp\" });\n    },\n    completedLogIn: function (data) {\n      localStorage.setItem(\"isAuth\", true);\n      localStorage.setItem(\"username\", data.username);\n      localStorage.setItem(\"token_access\", data.token_access);\n      localStorage.setItem(\"token_refresh\", data.token_refresh);\n      alert(\"Autenticación Exitosa\");\n      this.verifyAuth();\n    },\n    completedSignUp: function (data) {\n      alert(\"Registro Exitoso\");\n      // this.completedLogIn(data);\n    },\n    loadHome: function () {\n      this.$router.push({ name: \"home\" });\n    },\n    logOut: function () {\n      localStorage.clear();\n      alert(\"Sesión Cerrada\");\n      this.verifyAuth();\n    },\n    loadIps: function () {\n      this.$router.push({ name: \"ips\" });\n    },\n  },\n  created: function () {\n    this.verifyAuth();\n  },\n};\n</script>\n\n\n\n\n\n\n<style>\nbody {\n  margin: 0 0 0 0;\n}\n.header {\n  margin: 0%;\n  padding: 0;\n  width: 100%;\n  height: 10vh;\n  min-height: 100px;\n  background-color: #283747;\n  color: #e5e7e9;\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n}\n.header h1 {\n  width: 20%;\n  text-align: center;\n}\n.header nav {\n  height: 100%;\n  width: 20%;\n  display: flex;\n  justify-content: space-around;\n  align-items: center;\n  font-size: 20px;\n}\n.header nav button {\n  color: #e5e7e9;\n  background: #283747;\n  border: 1px solid #e5e7e9;\n  border-radius: 5px;\n  padding: 10px 20px;\n}\n.header nav button:hover {\n  color: #283747;\n  background: #e5e7e9;\n  border: 1px solid #e5e7e9;\n}\n\n.main-component {\n  height: 75vh;\n  margin: 0%;\n  padding: 0%;\n  background: #fdfefe;\n}\n\n/* .footer {\n  margin: 0;\n  padding: 0;\n  width: 100%;\n  height: 10vh;\n  min-height: 100px;\n  background-color: #283747;\n  color: #e5e7e9;\n}\n.footer h2 {\n  width: 100%;\n  height: 100%;\n\n  display: flex;\n  justify-content: center;\n  align-items: center;\n} */\n</style>"],"sourceRoot":""}]}